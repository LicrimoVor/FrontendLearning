  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:vite": "vite -c config/vite.config.ts",
    "start:dev": "npx concurrently \"npm start\" \"npm run start:dev:server\"",
    "start:dev:vite": "npx concurrently \"npm run start:vite\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack --env mode=production apiUrl=http://81.90.180.165:8010/",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "cypress open -C config/cypress.config.ts",
    "test:ui": "npx loki test --host host.docker.internal",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:ok": "npx loki approve",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "prepare": "husky install",
    "remove-features": "npx ts-node ./scripts/remove-feature.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint"
  },
